cppHeaderFileInclude {
  \.h$
  \.inc$
}

cppSrcFileInclude {
  \.cpp$
}

generatedFileExclude {
  gmock/
  llvm/
  src/arm-linux-jni/
  src/main/native/cpp/jni/
}

includeGuardRoots {
  src/main/native/cpp/
  src/main/native/include/
  src/test/native/cpp/
}

includeOtherLibs {
  ^llvm/
  ^support/
}
